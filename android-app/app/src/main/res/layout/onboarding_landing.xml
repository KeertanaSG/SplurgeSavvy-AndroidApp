<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/landingLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EADDCF">

    <ImageView
        android:id="@+id/splurgesavv"
        android:layout_width="270dp"
        android:layout_height="293dp"
        android:layout_marginTop="72dp"
        android:src="@drawable/logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/rectangle_2"
        android:layout_width="348dp"
        android:layout_height="344dp"
        android:layout_marginTop="316dp"
        android:background="@drawable/rectangle_shape"
        android:foreground="@drawable/rectangle_border"
        app:layout_constraintBottom_toTopOf="@+id/business_an"
        app:layout_constraintDimensionRatio="H,5:6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.095"/>

    <ImageView
        android:id="@+id/business_an"
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:layout_marginBottom="192dp"
        android:src="@drawable/business_an"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>
